/*sprites icons*/
@{{name}}sprite-url: url({{spritesheet.escaped_image}});
{{#sprites}}
@icon-{{name}}-position: {{px.offset_x}} {{px.offset_y}};
@icon-{{name}}-width: {{px.width}};
@icon-{{name}}-height: {{px.height}};
{{/sprites}}

.icon {
background-image: url({{spritesheet.escaped_image}});
}
{{#sprites}}
.icon-{{name}} {
    background-position: {{px.offset_x}} {{px.offset_y}};
    width: {{px.width}};
    height: {{px.height}};
}
{{/sprites}}
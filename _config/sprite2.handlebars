/*sprites icons2*/
@{{name}}sprite-url: url({{spritesheet.escaped_image}});
{{#sprites}}
@icon2-{{name}}-position: {{px.offset_x}} {{px.offset_y}};
@icon2-{{name}}-width: {{px.width}};
@icon2-{{name}}-height: {{px.height}};
{{/sprites}}

.icon2 {
background-image: url({{spritesheet.escaped_image}});
}
{{#sprites}}
.icon2-{{name}} {
    background-position: {{px.offset_x}} {{px.offset_y}};
    width: {{px.width}};
    height: {{px.height}};
}
{{/sprites}}